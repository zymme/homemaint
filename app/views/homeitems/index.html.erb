
 <div class="container theme-showcase" role="main">

   <div class="jumbotron">
        &nbsp;
        <br />
        <h3>Home Maintenance Web Application!</h3>
        <!-- <p>This our home maintenance / project listing web application! Use it at your own risk!</p> -->
   </div>

   <div class="page-header">
     Current List
   </div>

   <div class="row">
     <div class="col-md-9">
          <table class="table table-striped">
            <thead>
              <tr>
                <th>Name</th>
                <th>Status</th>
                <th>Stagetime</th>
                <th>UserId</th>
                <th>Description</th>
              </tr>
            </thead>
            <tbody>
              <% @homeitems.each do |homeitem| %>
                <tr>
                  <td><%= homeitem.name %></td>
                  <td><%= homeitem.status %></td>
                  <td><%= homeitem.stagetime %></td>
                  <td><%= homeitem.userid %></td>
                  <td><%= homeitem.desc %></td>
                  <!-- <td><%= link_to 'Show', homeitem %></td>
                  <td><%= link_to 'Edit', edit_homeitem_path(homeitem) %></td>
                  <td><%= link_to 'Destroy', homeitem, method: :delete, data: { confirm: 'Are you sure?' } %></td> -->
                </tr>
              <% end %>
            </tbody>
          </table>
        </div>
   </div>
   <%=link_to 'New Homeitem', new_homeitem_path, class: "btn btn-primary" %>

 </div>

